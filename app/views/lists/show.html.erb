<div class="container">
<h2><%= @list.name %></h2>
<% @list.bookmarks.each do |bookmark| %>
  <h4><%= bookmark.movie.title %></h4>
  <p><%= bookmark.movie.overview %></p>
  <p><%= bookmark.movie.rating %></p>
  <img src=<%= bookmark.movie.poster_url %> alt="">
<%= link_to "delete", bookmark_path(bookmark), method: :delete, data: { confirm: "Are you sure?" }  %>
<% end %>
<%= link_to "Back to List", lists_path, class: 'btn btn-primary'  %>
<%= link_to "Add Movie", new_list_bookmark_path(@list), class: 'btn btn-danger'  %>
</div>
